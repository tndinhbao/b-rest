{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "semi": 2,
    "linebreak-style": ["error", "windows"],
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "no-plusplus": 0,
    "comma-dangle": ["error", { "functions": "ignore" }],
    "func-names": 0,
    "object-curly-newline": 0,
    "arrow-parens": 0
  }
}
